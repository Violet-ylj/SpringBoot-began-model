<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ylj.beganmodel.demos.dao.PeopleDao">


    <select id="searchPeople" resultType="com.ylj.beganmodel.demos.Entity.People">
        select *
        from people
        where id = #{id}
    </select>

</mapper>